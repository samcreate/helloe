@import 'jeet'

@keyframes fadeIn { from { opacity:0; } to { opacity:1; } }

.home-container
	color white
	padding-top: 0px;
	min-height: 300px;
	overflow: auto;
	position: relative;
	align(both)
	width 89%
	+below(500px)
		transform: initial;
		position: relative;
		top: initial;
		left: inherit;
		padding-top: 40px;
	ul li
		background-repeat: no-repeat
		background-size: contain
		background-position: left
		width: 25%
		height: 100px
		transition: all .3s ease, opacity 4s ease
		span(1/4)
